{
    "manifest_version": 3,
    "name": "Tracker",
    "version": "1.0",
    "description": "Adds a problem to a specified GitHub Repository",

    "permissions": ["activeTab", "scripting", "storage"],
    "host_permissions": ["https://leetcode.com/problems/*"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon.png"
        }
    },
    "icons": {
        "16": "icons/icon.png"
    }
}