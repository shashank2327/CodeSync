{
  "manifest_version": 3,
  "name": "CodeSync",
  "version": "1.0",
  "description": "Adds CP problems from platform to github directly.",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["*://*.leetcode.com/problems/*",
    "*://*.codeforces.com/problemset/problem/*",
    "*://*.codeforces.com/contest/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon.png"
    }
  },
  "icons": {
    "16": "icons/icon.png"
  }
}
